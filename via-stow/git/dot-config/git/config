[alias]
    st = status
    com = commit -a
    amend = commit -a --amend
    co = checkout
    hash = rev-parse HEAD
    graph = log --graph --all --oneline --decorate
    rollback = checkout --
    unstage = reset --
    updiff = log --branches --not --remotes --oneline
[push]
    default = simple
[icdiff]
    options = --line-numbers
[init]
	defaultBranch = main
